<%if $followings.length > 0%>
	<% $followings.each do |following|%>
		<div class="col-xs-6 col-sm-4 col-lg-3">
			<%=render partial: "partials/avatar", locals: {resource: following}%>
			<center>
				<h3 class="hamurz main-color"><b><%= following.username.downcase %></b></h3>
			</center>
		</div>
	<%end%>
<%else%>
	<h4><%=hint('No followings here')%></h4>
<%end%>